{
  "name"       : "deepser/api-php",
  "type"       : "library",
  "description": "Deepser API PHP library",
  "keywords"   : ["Deepser", "API", "Help Desk", "Service Desk", "CRM"],
  "homepage"   : "https://github.com/deepser/api-php",
  "license"    : "MIT",

  "authors": [
    {
      "name"    : "Serman Nerjaku",
      "email"   : "serman.nerjaku@into-deep.com",
      "homepage": "https://www.deepser.com",
      "role"    : "Developer"
    }
  ],

  "support": {
    "issues": "https://github.com/deepser/api-php/issues",
    "source": "https://github.com/deepser/api-php"
  },

  "require": {
    "php": ">=7.0",
    "guzzlehttp/guzzle": "^5.3|^6.3"
  },

  "require-dev": {
    "guzzlehttp/guzzle": "^5.3|^6.3"
  },
  "autoload": {
    "psr-4": {
      "Deepser\\": "src/"
    }
  },

  "config": {
    "preferred-install": "dist"
  },

  "extra": {
    "branch-alias": {
      "dev-master": "1.0.0-dev"
    }
  }
}